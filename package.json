{
  "name": "magnet-plugin-function",
  "version": "0.0.8",
  "description": "Magnet plugin for function support",
  "engines": {
    "node": ">=7.6.0"
  },
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "babel --presets es2015 -d lib/ src/",
    "format": "prettier --single-quote --no-bracket-spacing --trailing-comma es5 --write \"{src/**/*.js}\" && eslint --fix \"{src/**/*.js}\"",
    "lint": "eslint src/**/*.js",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register test/setup/node.js test/unit/**/*.js"
  },
  "contributors": [
    "Ivan Santos <ivan.santos@liferay.com>"
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/wedeploy/magnet-plugin-function.git"
  },
  "dependencies": {
    "metal": "^2.6.4",
    "metal-assertions": "^2.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "eslint": "^3.17.1",
    "eslint-config-liferay": "^1.0.1",
    "magnet": "latest",
    "mocha": "^3.2.0",
    "prettier": "^1.2.2"
  }
}
